algoritmo "semnome"
// Função :
// Autor :
// Data : 26/01/2018
// Seção de Declarações
var
n, v, c: inteiro
Procedimento Calc(a, b: Inteiro)
inicio
             c <- a + b
             Escreva (c)
Fimprocedimento
inicio
      Escreva ("Digite um número: ")
      Leia (n)
      Escreva ("Digite outro número: ")
      Leia (v)
      Calc (N, V)
fimalgoritmo
